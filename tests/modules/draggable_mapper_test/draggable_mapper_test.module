<?php

/**
 * Implements hook_page_attachments().
 */
function draggable_mapper_test_page_attachments(array &$attachments) {
  $attachments['#attached']['library'][] = 'jquery_ui_draggable/draggable';
  $attachments['#attached']['library'][] = 'jquery_ui_droppable/droppable';
  $attachments['#attached']['library'][] = 'jquery_ui_resizable/resizable';
}